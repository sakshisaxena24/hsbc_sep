<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegistrationServlet</servlet-name>
  	<display-name>RegistrationServlet</display-name>
  	<description></description>
  	<servlet-class>com.facebookweb.controller.RegistrationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>facebookcontroller</servlet-name>
  	<display-name>facebookcontroller</display-name>
  	<description></description>
  	<servlet-class>com.facebookweb.controller.facebookcontroller</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Register</servlet-name>
  	<display-name>Register</display-name>
  	<description></description>
  	<servlet-class>com.facebookwebapp.controller.Register</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>login</servlet-name>
  	<display-name>login</display-name>
  	<description></description>
  	<servlet-class>com.facebookwebapp.controller.login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>friendlist</servlet-name>
  	<display-name>friendlist</display-name>
  	<description></description>
  	<servlet-class>com.facebookwebapp.controller.friendlist</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>friends</servlet-name>
  	<display-name>friends</display-name>
  	<description></description>
  	<servlet-class>com.facebookwebapp.controller.friends</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegistrationServlet</servlet-name>
  	<url-pattern>/RegistrationServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>facebookcontroller</servlet-name>
  	<url-pattern>/facebookcontroller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Register</servlet-name>
  	<url-pattern>/Register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>friendlist</servlet-name>
  	<url-pattern>/friendlist</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>friends</servlet-name>
  	<url-pattern>/friends</url-pattern>
  </servlet-mapping>
</web-app>
